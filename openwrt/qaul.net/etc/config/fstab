config 'global'
	option	anon_swap	'0'
	option	anon_mount	'0'
	option	auto_swap	'1'
	option	auto_mount	'1'
	option	delay_root	'5'
#	option	delay_root	'0'
	option	check_fs	'0'

config 'mount'
	option 'device' '/dev/sda1'
	option 'options' 'rw,sync'
	option 'enabled_fsck' '0'
	option 'enabled' '1'
	option 'target' '/mnt/share'

